{
    "name": "szemul/queue-worker",
    "description": "Queue processor / worker command for symfony console",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0",
        "ext-pcntl": "*",
        "symfony/console": "^v5.3.6"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0.0",
        "jetbrains/phpstorm-attributes": "^1.0",
        "mockery/mockery": "^1.0",
        "phpstan/phpstan": "^0.12.80",
        "phpunit/phpunit": "^9.5.5",
        "szemul/helper": "^1.0.0",
        "szemul/error-handler": "^1.0.0"
    },
    "autoload": {
        "psr-4": {
            "Szemul\\QueueWorker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Szemul\\QueueWorker\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
